{
  "name": "express-api",
  "version": "1.0.0",
  "author": "Akitsuyoshi",
  "license": "MIT",
  "description": "web api for blockchain",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon src/index.js",
    "test": "jest src",
    "coverage": "jest --collectCoverageFrom=src/**.js --collectCoverageFrom=!src/index.js --coverage src"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "./node_modules/.bin/eslint --fix",
      "git add"
    ]
  },
  "keywords": [
    "blockchain",
    "express"
  ],
  "devDependencies": {
    "eslint": "^5.6.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^2.6.2",
    "husky": "^1.0.0-rc.15",
    "lint-staged": "^7.3.0",
    "prettier": "^1.14.3"
  },
  "dependencies": {
    "crypto-js": "^3.1.9-1",
    "express": "^4.16.3",
    "jest": "^23.6.0",
    "level": "^4.0.0",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.4"
  }
}
