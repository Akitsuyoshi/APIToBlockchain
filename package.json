{
  "name": "express-api",
  "version": "1.0.0",
  "author": "Akitsuyoshi",
  "license": "MIT",
  "description": "web api for blockchain",
  "main": "index.js",
  "scripts": {
    "start": "node src/api/index.js",
    "dev": "nodemon src/api/index.js",
    "test": "mocha src/test/test.js",
